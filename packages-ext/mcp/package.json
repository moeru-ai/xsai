{
  "name": "@xsai-ext/mcp",
  "type": "module",
  "version": "0.1.0-beta.9",
  "private": true,
  "description": "extra-small AI SDK for Browser, Node.js, Deno, Bun or Edge Runtime.",
  "author": "Moeru AI",
  "license": "MIT",
  "homepage": "https://xsai.js.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moeru-ai/xsai.git",
    "directory": "packages-ext/mcp"
  },
  "bugs": "https://github.com/moeru-ai/xsai/issues",
  "keywords": [
    "xsai",
    "openai",
    "ai"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./sse": "./src/sse.ts",
    "./stdio": "./src/stdio.ts"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "exports": {
      ".": {
