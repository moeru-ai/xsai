        "types": "./dist/sse.d.ts",
        "default": "./dist/sse.js"
      },
      "./stdio": {
        "types": "./dist/stdio.d.ts",
        "default": "./dist/stdio.js"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "catalog:"
  },
  "dependencies": {
    "@xsai/shared-chat": "workspace:"
