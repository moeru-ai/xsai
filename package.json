    "@types/node": "^22.12.0",
    "@vitest/coverage-v8": "3.0.4",
    "automd": "^0.3.12",
    "bumpp": "^10.0.1",
    "eslint": "^9.19.0",
    "jiti": "^2.4.2",
    "pkgroll": "^2.6.1",
    "tsx": "^4.19.2",
    "vitest": "^3.0.4"
  }
}
