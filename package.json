{
  "name": "@xsai/workspace",
  "type": "module",
  "version": "0.1.0-beta.5",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "repository": "github:moeru-ai/xsai",
  "scripts": {
    "build": "corepack pnpm -r build",
    "build:watch": "corepack pnpm -r build:watch",
